


<div class="container">
  <div class="row justify-content-center">
    <div class="col-12">
      <h4>Add a Friend Group</h4>
      <%= simple_form_for [ @friend_group ] do |f| %>
        <%= f.input :price %>
        <%= f.input :description %>
        <%= f.input :title %>
        <%= f.input :address %>
        <%= f.input :photo, as: :file %>
    </div>
  </div>
  <li class= " btn btn-warning ml-2">
    <%= link_to 'Back', friend_groups_path, class: "a" %>
  </li>
        <%= f.submit "Add!", class: "btn btn-warning" %>
      <% end %>
</div>
